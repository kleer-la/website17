<script>

    $(document).ready(function(){
      $('.suscription-modal').on('shown.bs.modal', function (e) {
        $("#suscriptionModalEmail").focus();
      });
    });

    //Para evitar el cambio de tamaño de background al hacer scroll en mobile cuando se esconde la barra de direcciones
    $(window).resize(function () {
      resizeBackground();
    });
    function resizeBackground() {
      if ((/Android|iPhone|iPad|iPod|BlackBerry/i).test(navigator.userAgent || navigator.vendor || window.opera)) {
        if (screen.width > screen.height) {
          barHeight = 0;
        } else {
          barHeight = 60;
        }
        $("#slider").height(screen.height - barHeight);
      }
    }
    resizeBackground();

    // Cierra el sidebar menu
    function closeSidebar(e) {
      e.preventDefault();
      $("#sidebar-wrapper").toggleClass("active");
    }

    $("#menu-close").click(function (e) {
      closeSidebar(e);
    });
    // Abre el sidebar menu
    $("#menu-toggle").click(function (e) {
      e.preventDefault();
      $("#sidebar-wrapper").toggleClass("active");
    });
    // Efecto de animación de iconos
    window.sr = ScrollReveal();
    sr.reveal('.sr-services', {
      duration: 1000,
      scale: 0.3,
      distance: '100px',
      delay: 200
    }, 200);
    sr.reveal('.sr-button', {
      duration: 1000,
      scale: 0.3,
      delay: 500
    });
    sr.reveal('.sr-contact', {
      duration: 1000,
      scale: 0.3,
      distance: '0px',
      delay: 200
    }, 300);

    // Scroll animado a las secciones la página
    function scrollTo(anchor){
          var target = $(anchor);
          if (target.length) {
            $('html,body').animate({
              scrollTop: target.offset().top
            }, 1000);
            return false;
          }
    };

    // Link to-top aparece cuando hacemos scroll
    var fixed = false;
    $(document).scroll(function () {
      if ($(this).scrollTop() > 250) {
        if (!fixed) {
          fixed = true;
          $('#to-top').show("slow", function () {
            $('#to-top').css({
              position: 'fixed',
              display: 'block'
            });
          });
        }
      } else {
        if (fixed) {
          fixed = false;
          $('#to-top').hide("slow", function () {
            $('#to-top').css({
              display: 'none'
            });
          });
        }
      }
    });

    $('.carousel').carousel({
      interval: 5000
    })

</script>

