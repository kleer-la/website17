<section id="service-takeaways" >
  <div class="takeaways container">
    <h3 class="text-center mb-5 common-subtitle">
      <% if @service.titles %>
      <%=  @service.titles[0] %>
      <% else %>
      ¿Qué obtendrás con el <%=  @service.name %>?
      <% end %>
    </h3>
    <div class="row justify-content-center">
      <div class="col-md-10">
        <%= @markdown_renderer.render( @service.takeaways.to_s) %>
      </div>
    </div>
    <img src="https://kleer-images.s3.sa-east-1.amazonaws.com/website-assets/rustic_circleb.svg" class="desktop_figure course-learnings__rustic-circle" />
    <img src="https://kleer-images.s3.sa-east-1.amazonaws.com/website-assets/green_circle2.svg" class="desktop_figure course-learnings__green-circle" />
  </div>
</section>
