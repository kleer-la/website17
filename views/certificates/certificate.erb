<section class="certificates-form-container">
  <h1><%= t("certificate-form.title") %></h1>
  <h2 class=""><%= t("certificate-form.subtitle") %></h2>

  <div class="mb-3 form">
    <object data="<%= image_url%>" type="application/pdf" width="100%" height="600">
        <p>It appears you don't have a PDF plugin for this browser.
           You can <a href="<%= image_url%>">click here to download the PDF file.</a></p>
    </object>
  </div>

  <img
    class="desktop_figure skyblue-semicircle"
    src="<%= cdn('website-assets/skyblue_semicircle.svg') %>">
  <img
    class="desktop_figure rustic-circle"
    src="<%= cdn('website-assets/rustic_circle2.svg') %>"/>
  <img
    class="desktop_figure yellow-oval"
    src="<%= cdn('website-assets/yellow_oval2.svg') %>"/>
  <img
    class="desktop_figure red-bean"
    src="<%= cdn('website-assets/red_bean.svg') %>"/>
</section>
