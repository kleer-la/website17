  resizeBackground();
  Para evitar el cambio de tamaÃ±o de background al hacer scroll en mobile cuando se esconde la barra de direcciones%>
<script>
$(window).resize(function () {
  resizeBackground();
});
function resizeBackground() {
  if ((/Android|iPhone|iPad|iPod|BlackBerry/i).test(navigator.userAgent || navigator.vendor || window.opera)) {
    if (screen.width > screen.height) {
      barHeight = 0;
    } else {
      barHeight = 60;
    }
    $("#slider").height(screen.height - barHeight);
  }
}
resizeBackground();

$('.carousel').carousel({
  interval: 5000
})
</script>