<%
  if subitem.tr("\n", "") != '' && subitem != "" && subitem != " \n"

%>

  <a
    class="course-program__item"
    data-bs-toggle="collapse"
    href="#item<%=index %>"
    role="button"
    aria-expanded="false"
    aria-controls="item<%=index %>">

    <%= item %>
    <img src="/app/img/icons/arrow.svg"/>
  </a>
  <div class="collapse course-program__specific-item" id="item<%=index %>">
    <%= subitem %>
  </div>
<%else%>
  <div
    class="course-program__item-nocolaps"
    data-bs-toggle="collapse"
    href="#item<%=index %>"
    role="button"
    aria-expanded="false"
    aria-controls="item<%=index %>">

    <%= item %>
  </div>
<%end%>

